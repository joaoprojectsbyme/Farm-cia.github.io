    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    
    <!-- Custom JavaScript -->
    <script>
        // Initialize AOS animation
        AOS.init({
            duration: 800,
            easing: 'ease-in-out',
            once: true
        });
        
        // Quiz Script
        document.addEventListener('DOMContentLoaded', function() {
            const quizQuestions = [
                {
                    question: "Qual é a maneira correta de armazenar a maioria dos medicamentos?",
                    options: [
                        "No banheiro, para fácil acesso",
                        "Em local seco, fresco e protegido da luz solar",
                        "No congelador, para maior durabilidade",
                        "Dentro do carro, para ter sempre à mão"
                    ],
                    correct: 1,
                    feedback: "A maioria dos medicamentos deve ser armazenada em local seco, fresco e protegido da luz solar direta. O banheiro não é recomendado devido à umidade, e temperaturas extremas (como no congelador ou carro) podem danificar os medicamentos."
                },
                {
                    question: "O que fazer quando esquecer de tomar uma dose do medicamento?",
                    options: [
                        "Tomar duas doses na próxima vez",
                        "Tomar assim que lembrar, se não estiver perto da próxima dose",
                        "Parar de tomar o medicamento completamente",
                        "Nenhuma das alternativas"
                    ],
                    correct: 1,
                    feedback: "Se você esquecer uma dose, tome assim que lembrar, a menos que esteja perto do horário da próxima dose. Nunca tome duas doses para compensar. Alguns medicamentos têm protocolos específicos - consulte sempre seu médico ou farmacêutico."
                },
                {
                    question: "Qual destes medicamentos NÃO deve ser misturado com álcool?",
                    options: [
                        "Paracetamol",
                        "Dipirona",
                        "Omeprazol",
                        "Todos os acima"
                    ],
                    correct: 3,
                    feedback: "Todos esses medicamentos podem ter interações com álcool. O paracetamol combinado com álcool aumenta o risco de danos ao fígado. A dipirona pode potencializar os efeitos sedativos do álcool. Omeprazol pode aumentar a absorção de álcool. O ideal é evitar álcool quando estiver usando qualquer medicamento."
                },
                {
                    question: "Como descartar medicamentos vencidos corretamente?",
                    options: [
                        "No lixo comum",
                        "No vaso sanitário",
                        "Em pontos de coleta específicos",
                        "Queimando-os"
                    ],
                    correct: 2,
                    feedback: "Medicamentos vencidos devem ser descartados em pontos de coleta específicos, como farmácias participantes de programas de descarte ou Unidades Básicas de Saúde. Descarte no lixo comum ou vaso sanitário pode contaminar o meio ambiente."
                },
                {
                    question: "Quais são sinais de uma possível reação alérgica a medicamentos?",
                    options: [
                        "Erupções cutâneas e coceira",
                        "Inchaço no rosto ou língua",
                        "Dificuldade para respirar",
                        "Todos os acima"
                    ],
                    correct: 3,
                    feedback: "Todos são sinais de possível reação alérgica. Erupções cutâneas e coceira podem indicar reação leve, enquanto inchaço facial e dificuldade respiratória são sinais de reação grave (anafilaxia) que requer atendimento médico imediato."
                }
            ];
            
            let currentQuestion = 0;
            let score = 0;
            let userAnswers = [];
            
            const questionElement = document.getElementById('quiz-question');
            const optionsElement = document.getElementById('quiz-options');
            const feedbackElement = document.getElementById('quiz-feedback');
            const prevButton = document.getElementById('prev-question');
            const nextButton = document.getElementById('next-question');
            const progressBar = document.getElementById('quiz-progress');
            const counterElement = document.getElementById('quiz-counter');
            const quizArea = document.getElementById('quiz-area');
            const resultsArea = document.getElementById('quiz-results');
            const scoreElement = document.getElementById('quiz-score');
            const messageElement = document.getElementById('quiz-message');
            const restartButton = document.getElementById('restart-quiz');
            
            function loadQuestion() {
                const question = quizQuestions[currentQuestion];
                questionElement.textContent = question.question;
                
                optionsElement.innerHTML = '';
                question.options.forEach((option, index) => {
                    const optionElement = document.createElement('div');
                    optionElement.className = 'quiz-option';
                    if (userAnswers[currentQuestion] === index) {
                        optionElement.classList.add('selected');
                    }
                    optionElement.textContent = option;
                    optionElement.dataset.index = index;
                    optionElement.addEventListener('click', selectOption);
                    optionsElement.appendChild(optionElement);
                });
                
                if (userAnswers[currentQuestion] !== undefined) {
                    showFeedback();
                } else {
                    feedbackElement.style.display = 'none';
                }
                
                prevButton.disabled = currentQuestion === 0;
                nextButton.textContent = currentQuestion === quizQuestions.length - 1 ? 'Finalizar' : 'Próxima';
                progressBar.style.width = `${(currentQuestion + 1) / quizQuestions.length * 100}%`;
                counterElement.textContent = `Pergunta ${currentQuestion + 1} de ${quizQuestions.length}`;
            }
            
            function selectOption(e) {
                const selectedOption = e.target;
                const options = document.querySelectorAll('.quiz-option');
                
                options.forEach(option => {
                    option.classList.remove('selected');
                });
                
                selectedOption.classList.add('selected');
                userAnswers[currentQuestion] = parseInt(selectedOption.dataset.index);
                showFeedback();
            }
            
            function showFeedback() {
                const question = quizQuestions[currentQuestion];
                const userAnswer = userAnswers[currentQuestion];
                
                if (userAnswer === undefined) return;
                
                feedbackElement.textContent = question.feedback;
                feedbackElement.className = 'quiz-feedback show ' + 
                    (userAnswer === question.correct ? 'correct' : 'incorrect');
                feedbackElement.style.display = 'block';
            }
            
            function nextQuestion() {
                if (userAnswers[currentQuestion] === undefined) {
                    alert('Por favor, selecione uma resposta antes de prosseguir.');
                    return;
                }
                
                if (currentQuestion < quizQuestions.length - 1) {
                    currentQuestion++;
                    loadQuestion();
                } else {
                    showResults();
                }
            }
            
            function prevQuestion() {
                if (currentQuestion > 0) {
                    currentQuestion--;
                    loadQuestion();
                }
            }
            
            function showResults() {
                score = 0;
                for (let i = 0; i < quizQuestions.length; i++) {
                    if (userAnswers[i] === quizQuestions[i].correct) {
                        score++;
                    }
                }
                
                scoreElement.textContent = `Você acertou ${score} de ${quizQuestions.length} perguntas`;
                
                if (score === quizQuestions.length) {
                    messageElement.textContent = 'Parabéns! Você demonstrou excelente conhecimento sobre o uso correto de medicamentos!';
                } else if (score >= quizQuestions.length * 0.7) {
                    messageElement.textContent = 'Muito bom! Você tem um bom conhecimento, mas ainda pode aprender mais.';
                } else if (score >= quizQuestions.length * 0.4) {
                    messageElement.textContent = 'Você acertou algumas questões, mas ainda há muito para aprender sobre medicamentos.';
                } else {
                    messageElement.textContent = 'Seu conhecimento sobre medicamentos precisa melhorar. Recomendamos que revise nossos materiais educativos.';
                }
                
                quizArea.style.display = 'none';
                resultsArea.style.display = 'block';
            }
            
            function restartQuiz() {
                currentQuestion = 0;
                score = 0;
                userAnswers = [];
                quizArea.style.display = 'block';
                resultsArea.style.display = 'none';
                loadQuestion();
            }
            
            prevButton.addEventListener('click', prevQuestion);
            nextButton.addEventListener('click', nextQuestion);
            restartButton.addEventListener('click', restartQuiz);
            
            // Contact Form
            const contactForm = document.getElementById('contactForm');
            contactForm.addEventListener('submit', function(e) {
                e.preventDefault();
                alert('Mensagem enviada com sucesso! Entraremos em contato em breve.');
                contactForm.reset();
            });
            
            // Load first question
            loadQuestion();
        });
    </script>
</body>
</html>
